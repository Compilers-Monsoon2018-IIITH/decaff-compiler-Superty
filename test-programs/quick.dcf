class Program {
  int input[100];
  void quick(int l, int r) {
    int i, temp;

    if (l >= r) {
      return;
    }

    i = l;
    for j = l, r - 1 {
      if (input[j] < input[r]) {
        temp = input[j];
        input[j] = input[i];
        input[i] = temp;
        i += 1;
      }
    }
    temp = input[r];
    input[r] = input[i];
    input[i] = temp;

    quick(l, i - 1);
    quick(i + 1, r);
  }
  
