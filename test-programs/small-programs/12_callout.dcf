class Program {
  int input[100];  
  int getInt() {
    int ret, input;
    ret = 0;
    for idx = 1, 2 {
      idx -= 1;

      input = callout("getchar");
      if (input >= 48 && input <= 57) {
        ret = 10*ret + input - 48;
      } else {
        break;
      }
    }
    return ret;
  }
  int main() {
    int n;
    n = getInt();
    callout("printf", "%d", n);
    for i = 0, n {
      input[i] = getInt();
      callout("printf", "%d ", input[i]);
    }
    return 0;
  }
}
