class Program {
  int input[100];
  int nextmax(int n) {
    int max, secmax;
    
    if (input[1] > input[0]) {
      max = input[1];
      secmax = input[0];
    } else {
      max = input[0];
      secmax = input[1];
    }

    for i = 2, n - 1 {
      if (input[i] > max) {
        secmax = max;
        max = input[i];
      } else {
        if (input[i] > secmax) {
          secmax = input[i];
        }
      }
    }
    return secmax;
  }

  int main() {
    
  }
}